export ZSH="$HOME/.oh-my-zsh" 
# adds chezmoi to path
export PATH="./bin:$PATH"
export TERM="xterm-256color"
export EDITOR='nvim'
export VISUAL='nvim'

source ~/.zplug/init.zsh

zplug "dracula/zsh", as:theme

plugins=(
	git
	zsh-autosuggestions
	zsh-syntax-highlighting
)

source $ZSH/oh-my-zsh.sh
zplug load

